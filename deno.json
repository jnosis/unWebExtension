{
  "importMap": "./import_map.json",
  "fmt": {
    "options": {
      "singleQuote": true,
      "proseWrap": "preserve"
    }
  },
  "lint": {
    "files": {
      "exclude": ["src/test/testdata/"]
    }
  },
  "tasks": {
    "main": "deno run ./src/main.ts",
    "test": "deno test --allow-write --allow-read --coverage=./coverage",
    "coverage": "deno coverage ./coverage"
  }
}
